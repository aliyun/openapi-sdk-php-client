{
    "name": "alibabacloud/client",
    "homepage": "https://www.alibabacloud.com/",
    "description": "Alibaba Cloud Client for PHP",
    "keywords": [
        "aliyun",
        "alibaba",
        "alibabacloud",
        "client",
        "sdk",
        "cloud"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "support": {
        "issues": "https://github.com/aliyun/openapi-sdk-php-client/issues",
        "source": "https://github.com/aliyun/openapi-sdk-php-client"
    },
    "authors": [
        {
            "name": "Alibaba Cloud SDK",
            "homepage": "http://www.alibabacloud.com",
            "email": "sdk-team@alibabacloud.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "clagiordano/weblibs-configmanager": "^1.0",
        "danielstjules/stringy": "^3.1",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/promises": "^1.3",
        "guzzlehttp/psr7": "^1.4",
        "mtdowling/jmespath.php": "^2.4"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-spl": "*",
        "ext-sockets": "*",
        "psr/cache": "^1.0",
        "drupal/coder": "^8.3",
        "league/climate": "^3.2.",
        "symfony/dotenv": "^3.4",
        "phpunit/phpunit": "^4.8",
        "phpunit/php-invoker": "^1.1",
        "mikey179/vfsStream": "^1.6",
        "symfony/var-dumper": "^3.4"
    },
    "suggest": {
        "ext-openssl": "Working with private distributions and verifying received messages",
        "ext-curl": "To send requests using cURL",
        "ext-sockets": "To use client-side monitoring"
    },
    "autoload": {
        "psr-4": {
            "AlibabaCloud\\Client\\": "src"
        },
        "files": [
            "src/Constants/Business.php",
            "src/Constants/ErrorCode.php",
            "src/Functions.php",
            "src/Load.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AlibabaCloud\\Client\\Tests\\": "tests/"
        },
        "classmap": [
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "AlibabaCloud\\Client\\Regions\\EndpointProvider::updateEndpointsFromJson",
            "./php-cs-fixer fix ./"
        ]
    }
}
